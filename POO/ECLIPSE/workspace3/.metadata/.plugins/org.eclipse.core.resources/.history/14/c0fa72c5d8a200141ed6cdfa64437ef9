package ejercicios;
//Â¿Cuales de las siguientes sentencias son validas para la declaracion de un array?:
public class Ejercicio1 {
	public static void main(String[] args) {
		int i = new int(30); // int i[] = new int[30];
		double d[ ] = new double[30];
		char[ ] r = new char(1..30); // char[ ] r = new char[1.30];
		int i[ ] = (3, 4, 3, 2); // int i[ ] = {3, 4, 3, 2};
		float f[ ] = {2.3, 4.5, 6.6};
		char[ ] c = new char(); // char[ ] c = new char[];
	}
}
